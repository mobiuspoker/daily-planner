{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:tray:default",
    "core:window:default",
    "opener:default",
    "notification:default",
    "autostart:default",
    "sql:default",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-load",
    "sql:allow-close",
    "global-shortcut:default",
    "fs:default",
    "dialog:default",
    "process:default"
  ]
}
